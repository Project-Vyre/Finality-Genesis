# EditorConfig is awesome: https://editorconfig.org

root = true

[*]
end_of_line = crlf
insert_final_newline = false

# Matches multiple files with brace expansion notation
# Set default charset
[*.{js,json,py}]
charset = utf-8

[*.{js,json}]
indent_style = space
indent_size = 2

[*.py]
indent_style = space
indent_size = 4